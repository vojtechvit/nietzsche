@model NietzscheBiography.WebSite.ViewModels.Places.Index
@{
    ViewBag.Title = "Places of " + Model.Participant.Name + " : Nietzsche Biography";
    ViewBag.ActiveParticipant = Model.Participant;
    ViewBag.Description = "";
}
@section Scripts
{
    @Scripts.Render("~/js/map")
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
}
<header class="page-header">
    <h1>Places of @Model.Participant.Name</h1>
</header>
<div class="well">
    <div id="map_canvas" data-participant-id="@Model.Participant.Id" class="map-canvas"></div>
</div>
<div id="radios" class="thumbnail map-legend">
    <fieldset>
        <legend>Event Occurrences</legend>
    </fieldset>
</div>

<!-- Modal -->
<div id="location-events-modal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3>Events</h3>
    </div>
    <div class="modal-body"></div>
    <div class="modal-footer">
        <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
    </div>
</div>